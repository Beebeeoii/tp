@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":QuickBookParser" as QuickBookParser LOGIC_COLOR
participant ":FindCommandParser" as FindCommandParser LOGIC_COLOR
participant "d:FindCommand" as FindCommand LOGIC_COLOR
participant ":ContainsNamesPredicate" as ContainsNamesPredicate LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("find n/Bob")
activate LogicManager

LogicManager -> QuickBookParser : parseCommand("find n/bob")
activate QuickBookParser

create FindCommandParser
QuickBookParser -> FindCommandParser
activate FindCommandParser

FindCommandParser --> QuickBookParser
deactivate FindCommandParser

QuickBookParser -> FindCommandParser : parse("n/bob")
activate FindCommandParser

create FindCommand
FindCommandParser -> FindCommand
activate FindCommand
create ContainsNamesPredicate

FindCommand -> ContainsNamesPredicate
activate ContainsNamesPredicate
ContainsNamesPredicate --> FindCommand
deactivate ContainsNamesPredicate

FindCommand --> FindCommandParser : d
deactivate FindCommand

FindCommandParser --> QuickBookParser : d
deactivate FindCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
FindCommandParser -[hidden]-> QuickBookParser
destroy FindCommandParser

QuickBookParser --> LogicManager : d
deactivate QuickBookParser

LogicManager -> FindCommand : execute()
activate FindCommand
FindCommand -> Model : find()
activate Model
Model -> ContainsNamesPredicate: FilteredList()
activate ContainsNamesPredicate
ContainsNamesPredicate --> Model: return FilteredList
deactivate ContainsNamesPredicate

Model --> FindCommand: returns filteredList
deactivate Model

FindCommand --> LogicManager : return
deactivate FindCommand

[<--LogicManager
deactivate LogicManager
@enduml
